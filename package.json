{
  "name": "@unocss/monorepo",
  "version": "0.16.4",
  "private": true,
  "scripts": {
    "bench": "npm -C bench run bench",
    "build": "esno scripts/copy-files.ts && pnpm -r --filter !playground run build && pnpm -r run build-post",
    "dev": "nr stub",
    "stub": "pnpm -r --parallel run stub",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "nr lint -- --fix",
    "play": "npm -C playground run dev",
    "play:build": "pnpm run build && npm -C playground run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "size": "esmo scripts/size.ts",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.14.0",
    "@antfu/ni": "^0.12.0",
    "@iconify-json/carbon": "^1.0.12",
    "@iconify-json/logos": "^1.0.2",
    "@iconify-json/mdi": "^1.0.12",
    "@iconify-json/ph": "^1.0.2",
    "@iconify-json/ri": "^1.0.2",
    "@iconify-json/tabler": "^1.0.8",
    "@iconify-json/twemoji": "^1.0.2",
    "@iconify-json/uim": "^1.0.2",
    "@types/codemirror": "^5.60.5",
    "@types/fs-extra": "^9.0.13",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^17.0.0",
    "@types/ws": "^8.2.2",
    "@unocss/cli": "workspace:*",
    "@unocss/config": "workspace:*",
    "@unocss/core": "workspace:*",
    "@unocss/extractor-pug": "workspace:*",
    "@unocss/preset-attributify": "workspace:*",
    "@unocss/preset-icons": "workspace:*",
    "@unocss/preset-mini": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/preset-wind": "workspace:*",
    "@unocss/reset": "workspace:*",
    "@unocss/vite": "workspace:*",
    "@unocss/webpack": "workspace:*",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vueuse/core": "^7.3.0",
    "brotli-size": "^4.0.0",
    "bumpp": "^7.1.1",
    "codemirror": "^5.64.0",
    "codemirror-theme-vars": "^0.1.1",
    "eslint": "^8.5.0",
    "esno": "^0.13.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "gzip-size": "^6.0.0",
    "lz-string": "^1.4.4",
    "prettier": "^2.5.1",
    "rollup": "^2.61.1",
    "splitpanes": "^3.0.6",
    "terser": "^5.10.0",
    "tsup": "5.11.6",
    "typescript": "^4.5.4",
    "unbuild": "0.6.3",
    "unocss": "workspace:*",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.7.3",
    "vite-node": "^0.1.11",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-pages": "^0.19.4",
    "vitest": "^0.0.96",
